{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "printWidth": 100,
  "semi": true,
  "svelteAllowShorthand": true,
  "trailingComma": "all",
  "useTabs": false
}
